#include <iostream>

using namespace std;

int main()
{
	/*так как в задании сказано, что программа должна напечатать квадрат из N звездочек, но не задано число N,
	то пользователь должен ввести число N, а программа должна проверить может ли получится квадрат из этого N звездочек*/
	int N,a,b;
	cin >> N;
	b = 1;
	a = N;
	while (a >= 100) {
		a = a / 100;
		b = b * 100;
	}
	/*для того чтобы построить квадрат из N звездочек нужно найти квадратный корень из N,
	запишем значение корня из N в переменную с*/
	int i = 0;
	int c = 0;
	while (b >= 100){
		a = N / b;
		while ((a - ((c*20 + i) * i)) > 0){
			i++;
		}
		i--;
		N = N - ((c * 20 + i) * i) * b;
		c = c * 10 + i;
		b = b / 100;
		i = 0;
	}
	while ((N - ((c * 20 + i) * i)) >= 0) {
		i++;
	}
	i--;
	N = N - ((c * 20 + i) * i);
	c = c * 10 + i;
	/*если значение квадратного корня из N не целое число, то тогда значение переменной N после нахождения квадратного корня
	будет равно нулю*/
	if (N != 0) {
		cout << "невозможно построить квадрат";
	}
	else {
		for (int i = 1; i <= c; i++) {
			/*перемещение координаты по х*/
			for (int j = 1; j <= c; j++) {
				cout << "*";
			}
			/*переход на новую строку*/
			cout << endl;
		}
	}
	return 0;
}
