#include <iostream>

using namespace std;

int main()
{
//так как в условии задачи нам не даны числа, пользователь вводит их самостоятельно
	int a, b;
	cin >> a >> b;
//так как в условии не сказано какое число делится на какое, предположим, что первое число делится на второе
//так как числа могут быть нулевыми, надо проверить равно ли число b нулю и если равно вывести соответствующие сообщение
	if (b == 0) {
		cout << "на ноль делить нельзя";
	}
	else {
		cout << "частное = " << a / b;
	}
	return 0;
}
