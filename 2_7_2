#include <iostream>

using namespace std;

int main()
{
	/*так как в условиях задачи не задано число N, то пользователь должен ввести его самостоятельно*/
	int N;
	cin >> N;
	/*перемещение координаты по у*/
	for (int i = 1; i <= N; i++) {
		/*перемещение координаты по х*/
		for (int j = 1; j <= N; j++) {
			cout << "*";
		}
		/*переход на новую строку*/
		cout << endl;
	}
	return 0;
}
