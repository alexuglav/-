#include <iostream>

using namespace std;

int main()
{
/*так как в условии задачи не сказано до какаого члена последовательности мы должны производить действия,
то пользователь должен ввести это самостоятельно*/
	int n,a,s;
//переменная а будет хранить произведение, переменная s будет хранить сумму произведений.
	a = 1;
	s = 0;
	cin >> n;
	for (int i = 1; i <= n; i++) {
//каждое произведение будет состоять из элементов от i до 2*i
		for (int j = i; j <= 2*i; j++) {
			a = a * j;
		}
		s = s + a;
		a = 1;
	}
	cout << s;
	return 0;
}
