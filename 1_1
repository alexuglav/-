#include <iostream>

using namespace std;

int main()
{
//так как в условиях задачи не заданы три числа, значит пользователь вводит эти числа
	int max, a;
	cin >> max;
	for (int i = 1; i <= 2; i++) {
		cin >> a;
//при каждом вводе числа, программа будет проверять максимальное и записывать его в переменную max
		if (max < a) {
			max = a;
		}
	}
//выведем максималььное число
	cout << max;
	return 0;
}
